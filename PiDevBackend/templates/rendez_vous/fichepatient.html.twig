                    <div class="col-md-4 mb-3">
                        <label for="dinner" class="form-label visually-hidden">Dinner</label>
                        {{ form_row(form.dinner, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="col-md-4 mb-3">
                        <label for="snacks" class="form-label visually-hidden">Snacks</label>
                        {{ form_row(form.snacks, {'attr': {'class': 'form-control'}}) }}
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4 mb-3">
                        <label for="calories" class="form-label visually-hidden">Calories</label>
                        {{ form_row(form.calories, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="col-md-8 mb-3">
                        <label for="other" class="form-label visually-hidden">Other</label>
                        {{ form_row(form.other, {'attr': {'class': 'form-control'}}) }}
                    </div>
                </div>
                <button type="submit" class="btn btn-primary">Confirm</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
</section>




