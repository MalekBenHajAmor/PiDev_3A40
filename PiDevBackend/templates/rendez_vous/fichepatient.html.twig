  <!-- Form Section -->
<section id="book-form" class="book-form">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                {{ form_start(form) }}
                <br> <br>
                <div class="row">
                    <div class="col-md-4 mb-3">
                        <label for="weight" class="form-label visually-hidden">Weight</label>
                        {{ form_row(form.weight, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="col-md-4 mb-3">
                        <label for="muscle_mass" class="form-label visually-hidden">Muscle Mass</label>
                        {{ form_row(form.muscle_mass, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="col-md-4 mb-3">
                        <label for="height" class="form-label visually-hidden">Height</label>
                        {{ form_row(form.height, {'attr': {'class': 'form-control'}}) }}
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4 mb-3">
